<h1>Book holidays!</h1>
<p>This is the calendar</p>

<table border="1">
  <tr>
    <th>who</th>
    <th>from</th>
    <th>to</th>
  </tr>

  <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.who %></td>
        <td><%= booking.from %></td>
        <td><%= booking.to %></td>
      </tr>
  <% end %>
</table>

<%= link_to 'Request time off', new_booking_path %>